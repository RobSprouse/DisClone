{
     "name": "disclone",
     "description": "A simple app that can be used to chat online with your friends.",
     "version": "1.0.0",
     "main": "server/server.s",
     "scripts": {
          "start": "node server/server.js",
          "dev": "concurrently \"cd server && npm run watch\" \"cd client && npm run dev\"",
          "install": "cd server && npm i && cd ../client && npm i",
          "build": "cd client && npm run build",
          "format": "prettier --write ."
     },
     "repository": {
          "type": "git",
          "url": "git+https://github.com/RobSprouse/DisClone.git"
     },
     "author": "Robert Sprouse <drgstriker@aol.com> (https://github.com/RobSprouse)",
     "contributors": [
          {
               "name": "Contributor One",
               "email": "contributorone@email.com",
               "url": "https://github.com/contributorone"
          },
          {
               "name": "Contributor Two",
               "email": "contributortwo@email.com",
               "url": "https://github.com/contributortwo"
          }
     ],
     "license": "MIT",
     "bugs": {
          "url": "https://github.com/RobSprouse/DisClone/issues"
     },
     "homepage": "https://github.com/RobSprouse/DisClone#readme",
     "dependencies": {
          "concurrently": "^8.2.2",
          "prettier": "^3.2.4"
     }
}
